<p style="color: green"><%= notice %></p>

<h1>Scores</h1>

<div id="scores">
  <% @scores.each do |score| %>
    <%= render score %>
    <p>
      <%= link_to "Show this score", score_url(score, qstudent: params[:qstudent], qstudentid: params[:qstudentid]) %>
    </p>
  <% end %>
</div>

<%= link_to "New score", new_score_path(qstudent: params[:qstudent], qstudentid: params[:qstudentid]) %>
